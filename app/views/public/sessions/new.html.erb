<div class="col-md-6 col-sm-10 mx-2 mx-sm-auto shadow-lg rounded px-0 my-5 registrations pb-5" style="text-align: center;">
	<div class='pt-5', id="title">
		<p>
			<span>S</span>
			<span>I</span>
			<span>G</span>
			<span style ="margin-right:20px;">N</span>
			<span>U</span>
			<span>P</span>
		</p>
	</div>
<%= form_for(resource, as: resource_name, url: registration_path(resource_name)) do |f| %>
  <p><%= render "public/shared/error_messages", resource: resource %><p>
  <%- if devise_mapping.omniauthable? %>
    <%- resource_class.omniauth_providers.each do |provider| %>
     <p class="login"> <%= link_to "Twitterアカウントでログイン", omniauth_authorize_path(resource_name, provider), class:'fab fa-twitter col btn btn-funky-moon btn-rounded mb-5 mt-5', method: :post %></p>
    <% end %>
  <% end %>
  		  <br><p class="guest"><%= link_to 'ゲストログイン（閲覧用）', guest_sign_in_path, method: :post , class: "btn btn-primary btn-sm px-4 btn-rounded " %></p>
<% end %>
</div>


<%#= render "public/shared/links" %>
