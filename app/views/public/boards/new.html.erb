<h1>画像投稿フォーム</h1>
  <%= form_with model: @board do |f| %>
    <h4>画像</h4>
       <%= f.file_field :image, accept: "image/*", class: "w-100", id: "upload" , type: "file", name: "upload"  %>
    <h4>質問</h4>
      <%= f.text_field :headline %>
    <h4>内容</h4>
      <%= f.text_field :question %>

    <%= f.submit '投稿' %>
  <% end %>